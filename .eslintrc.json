{
  "globals": {
    "angular": false,
    "Socket": false,
    "Clipboard": false,
    "xssFilters": false,
    "moment": false
  },
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 2,
    "eqeqeq": 2,
    "strict": 2,
    "no-shadow": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "browser": true
  },
  "plugins": [
    "angular"
  ],
  "extends": "eslint:recommended"
}
